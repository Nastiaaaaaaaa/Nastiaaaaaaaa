#include <iostream>
#include <vector>
#include <cassert>

// Функция увеличения элементов вектора и тест для неё
void testIncreaseVector() {
    std::vector<int> vec = {1, 2, 3, 4, 5};
    std::vector<int> expected = {6, 7, 8, 9, 10};
    std::vector<int> result = increaseVector(vec, 5);

    assert(result == expected);
    std::cout << "Test Passed!" << std::endl;
}

int main() {
    testIncreaseVector();

    return 0;
}
